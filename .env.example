GATSBY_SITE_BASE_URL=

# Drupal website folder
SITE_FOLDER=multisite/ecip
GATSBY_SITE_FOLDER=multisite/ecip

# Initiatives API
GATSBY_INITIATIVES_API=

# Optional
DRAFT_PREVIEW=
BASIC_AUTH_USERNAME=
BASIC_AUTH_PASSWORD=
